{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/index.html"
        }
    ],
    "functions": {
        "backend/server.js": {
            "maxDuration": 10,
            "memory": 1024
        }
    }
}